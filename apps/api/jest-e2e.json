{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "coverageDirectory": "./coverage",
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "@src/(.*)$": "<rootDir>/src/$1",
    "@libs/(.*)$": "<rootDir>/src/libs/$1",
    "@tests/(.*)$": "<rootDir>/tests/$1"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
