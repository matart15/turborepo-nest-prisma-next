{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tsc": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:debug": "NODE_ENV=development nest start --watch --debug",
    "test": "NODE_ENV=test dotenv -e .env.test -- nest start",
    "stg": "NODE_ENV=production nest start",
    "prod": "NODE_ENV=production node dist/src/main",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate dev && prisma generate",
    "db:seed": "prisma db seed",
    "db:pull": "prisma db pull",
    "db:reset": "prisma migrate reset --force dev",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "console": "nest start --entryFile repl",
    "db:migrate:test": "dotenv -e .env.test yarn db:migrate",
    "db:seed:test": "dotenv -e .env.test yarn db:seed",
    "db:test:reset": "dotenv -e .env.test yarn db:reset",
    "jest": "dotenv -e .env -- jest --runInBand --verbose --coverage",
    "e2e": "dotenv -e .env.test cross-env NODE_ENV=test -- jest --config ./jest-e2e.json --verbose --runInBand",
    "generate:repository": "hygen repository_generator new",
    "generate:command_handler": "hygen command_handler new"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.22",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.22",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "4.6.1",
    "apollo-server-express": "^3.10.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.6.0",
    "nanoid": "3.3.4",
    "nestjs-i18n": "^9.2.1",
    "nestjs-pino": "^3.1.1",
    "nestjs-prisma": "^0.19.0",
    "nestjs-request-context": "^2.1.0",
    "nestjs-stripe": "^1.0.0",
    "oxide.ts": "^1.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "prisma": "^4.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^10.14.0",
    "typescript": "^4.3.5",
    "zod": "^3.19.1",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-transformer": "^0.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-custom": "^0.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hygen": "^6.2.11",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
